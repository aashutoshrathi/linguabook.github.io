{
  "name": "linguabook-components",
  "version": "1.0.0",
  "license": "Apache",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:js build:css",
    "build:js": "tsc",
    "build:css": "cpx \"src/**/*.{scss,gif,png,jpg}\" dist",
    "dev": "run-p dev:js dev:css",
    "dev:js": "tsc --watch",
    "dev:css": "yarn build:css --watch",
    "start": "yarn dev",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.6",
    "@chakra-ui/react": "^1.3.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "clsx": "^1.1.1",
    "country-code-lookup": "^0.0.19",
    "exenv": "^1.2.2",
    "framer-motion": ">= 3.10.3",
    "lingua-scraper": "0.16.2",
    "lodash": "^4.17.21",
    "query-string": "^7.0.0",
    "react-content-loader": "^6.0.2",
    "react-icons": "^4.2.0",
    "react-infinite-scroll-component": "^6.0.0",
    "react-responsive": "^8.2.0",
    "react-responsive-carousel": "^3.2.15",
    "react-router-dom": "^5.2.0",
    "readline": "^1.3.0",
    "recoil": "^0.1.3",
    "swr": "^0.5.3",
    "use-debounce": "^6.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "@types/exenv": "^1.2.0",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.7"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  }
}
